{
  "name": "Company",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string",
      "required": true,
      "length": 250
    },
    "Description": {
      "type": "string",
      "length": 1000
    },
    "image": {
      "type": "string",
      "length": 1000,
      "description": "File name of image"
    },
    "AddedAt": {
      "type": "date",
      "required": true,
      "postgresql": {
        "columnName": "added_at"
      }
    },
    "AddedById": {
      "type": "number",
      "required": true,
      "postgresql": {
        "columnName": "added_by_id"
      }
    },
    "ModifiedAt": {
      "type": "date",
      "required": true,
      "postgresql": {
        "columnName": "modified_at"
      }
    },
    "ModifiedById": {
      "type": "number",
      "required": true,
      "postgresql": {
        "columnName": "modified_by_id"
      }
    }
  },
  "validations": [],
  "relations": {
    "types": {
      "type": "hasOne",
      "model": "CompanyType",
      "foreignKey": "typeId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
